<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <LangVersion>7.1</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Krzysztof Jankowski</Authors>
    <Company>krzysztof.jankowski</Company>
    <Product>Extension for EFCoreLinqKit</Product>
    <PackageId>LinqKitEFCore.Extension.AutoSearchEntities</PackageId>
    <Version>0.0.2</Version>
    <NeutralLanguage>en</NeutralLanguage>
    <RepositoryUrl>https://github.com/janek9971/LinqKitEFCore.Extension.AutoSearchEntities</RepositoryUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
	    <PackageLicenseExpression>MIT</PackageLicenseExpression>
	    <PackageProjectUrl>https://github.com/janek9971/LinqKitEFCore.Extension.AutoSearchEntities</PackageProjectUrl>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2019.1.3" />
    <PackageReference Include="LinqKit.Microsoft.EntityFrameworkCore" Version="1.1.16" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="PredicateSearchProvider\CustomExpressionProviders\ICustomExpressions.cs">
      <SubType>Form</SubType>
    </Compile>
  </ItemGroup>

</Project>
